<?xml version="1.0" encoding="utf-8"?>
<structure xmlns="https://cmln.github.io/chameleon/schema/3.0/layout.rng">

	<grid class="title-sticky d-block d-lg-none container">
		<row>
			<cell>
				<component type="Html"><![CDATA[
					<div class="d-flex">
						<div class="align-self-center"><a class="sticky-title-logo" href="/wiki/Triple_Performance" title="Triple Performance"><img src="/skins/skin-neayi/favicon/logo-triple-performance-square.svg" alt="Triple Performance" class="d-lg-none"></a></div>
						<div class="align-self-center sticky-title-span"></div>
					</div>
					<div id="scrollbar-mobile" class="scrollbars"><div class="scrollbar-bg"></div></div>
				]]></component>
			</cell>
		</row>
	</grid>

	<component type="NavbarHorizontal">
		<component type="Html" position="head">
			<![CDATA[<div id="p-logo" class="navbar-brand p-logo" role="banner">
			<a href="/wiki/Triple_Performance" title="Triple Performance"><img src="/skins/skin-neayi/favicon/logo-triple-performance.svg" alt="Triple Performance" class="d-none d-lg-inline"><img src="/skins/skin-neayi/favicon/logo-triple-performance-mobile.svg" alt="Triple Performance" class="d-lg-none"></a>
		</div>]]></component>

		<component type="NavMenu" flatten="navigation"/>
		<component type="PageTools" class="flex-row my-auto" hideDiscussionLink="yes" buttons="" hideSelectedNameSpace="no" />
		<component type="SearchBar" class="order-first order-cmln-0 my-auto" position="right" buttons="go" placeholder="Rechercher" />
		<component type="PersonalTools" class="my-auto"/>
		<component type="Container" class=""><component type="Html">
			<![CDATA[<ul class="navbar-nav">
	@@productions@@

    @@thematiques@@

    @@additionalMenus@@

</ul>]]></component></component>
		<component type="Container" position="right" class=""><component type="Html">
			<![CDATA[<div class="neayi-square-button" id="neayi-search-button"><span class="material-icons" aria-hidden="true">search</span></div>
					<div class="neayi-square-button" id="neayi-add-button"><a class="stretched-link" href="/wiki/Help:CrÃ©er_une_page"><span class="material-icons" aria-hidden="true">add</span></a></div>]]>
			</component>
		</component>
		<component type="Container" position="right" class="create-profile"><component type="Html">
			<![CDATA[<div class="row align-items-center" id="neayi-createaccount" style="margin: 0; height: 100%;">
			 	<div class="col text-center">@@Connect-link@@</div>
			</div>]]></component></component>
	</component>

	<grid mode="fluid" class="flex-fill WikiMainContent">
		<row>
			<cell>
				<component type="Html"><![CDATA[<div id="scrollbar" class="scrollbars"><div class="scrollbar-bg"></div></div>
				<div id="neayi-searchform"><span id="searchform-close" class="material-icons" aria-hidden="true">close</span></div>]]></component>
			</cell>
		</row>

		<row>
			<cell>
				<component type="SiteNotice"/>
			</cell>
		</row>

		<row>
			<cell>
				<component type="Container" class="row mainContainer">
					<component type='Html'><![CDATA[<div class="col-12 hero-portail" style="display:none"><div class="row pt-md-5"><div class="hero-portail-img"><img></div>
<div class="black-layer"></div>
<div class="col-12 mx-auto hero-titles">
<h1 class="text-white mt-md-3"></h1>
<h2 class="text-white mt-md-5"></h2>
</div></div></div>]]></component>

					<component type="Container" class="leftSide col-auto"></component>
					<component type="Container" class="centralDiv col">
					<component type='Html'><![CDATA[]]></component>
						<component type="MainContent" class=""></component>
					</component>
					<component type="Container" class="rightSide col-auto d-lg-block d-none">
					<component type='Html'><![CDATA[@@interaction-bloc@@]]></component>

					</component>
				</component>
			</cell>
		</row>
	</grid>

	<grid class="social-sticky container text-center py-2">
		<row>
			<cell class="d-none d-md-block text-left">
				<component type="Html"><![CDATA[@@suggest-a-page@@]]></component>
			</cell>
			<cell class="text-md-right">
				<component type="Html"><![CDATA[@@share-on-social-networks@@]]></component>
			</cell>

		</row>
	</grid>

	<grid class="footer py-5">
		<row>
			<cell>
				<component type="Html"><![CDATA[
            <div class="row align-items-center">
                <div class="col-md-10 pb-4 offset-xl-1">
                    <img src="/skins/skin-neayi/favicon/logo-triple-performance.svg" class="img-fluid mb-md-0 mb-3">
                </div>
			</div>
			<div class="row">
                @@footer@@
            </div>]]></component>
			</cell>
		</row>

		<row>
			<cell>
				<component type="NavbarHorizontal" collapsible="no" class="small mb-2" >
					<component type="Toolbox" flatten="no" class="dropup">
                        <modification type="ShowOnlyFor" group="sysop" />
                    </component>
					<component type="LangLinks" flatten="no" class="dropup"/>
				</component>
			</cell>
		</row>

	</grid>

	<component type="Html"><![CDATA[@@modal-dialogs@@]]></component>
</structure>
